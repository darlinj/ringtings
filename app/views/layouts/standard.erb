<head>
  <script src="/javascripts/jquery.js" type="text/javascript"></script>
  <script src="/javascripts/jquery-ui.js" type="text/javascript"></script>
  <script src="/javascripts/jrails.js" type="text/javascript"></script>
  <script src="/javascripts/application.js" type="text/javascript"></script>
  <link rel="stylesheet" href="/css/blueprint/screen.css" type="text/css" media="screen, projection">
  <link rel="stylesheet" href="/css/blueprint/print.css" type="text/css" media="print">	
  <link rel="stylesheet" href="/css/ringtings.css" type="text/css" media="screen,projection">	
  <!--[if lt IE 8]><link rel="stylesheet" href="css/blueprint/ie.css" type="text/css" media="screen, projection"><![endif]-->
</head>
<body id="<%= @tab %>">
  <div class="container">
    <div class="span-24 last">
      <div class="top_section">
        <%= render :partial => '/shared/breadcrumbs' %>
        <div class="page_header">
          <a href="/"><img src="/images/ringtings.gif" /></a>
        </div>
        <ul id="tabnav">
          <li class="home"><a href="/">home</a></li>
          <li class="more"><a href="">more info</a></li>
          <li class="tryit"><a href="/callplans">try it free</a></li>
          <li class="signup"><a href="/sign_up">sign up</a></li>
        </ul>
      </div>
    </div>
    <div class="span-24 last">
      <div class="main_content" >
        <% if flash[:success] %>
          <div class="success"><%= flash[:success] %></div>
        <% end %>
        <% if flash[:failure] %>
          <div class="failure"><%= flash[:failure] %></div>
        <% end %>
        <% if flash[:notice] %>
          <div class="notice"><%= flash[:notice] %></div>
        <% end %>
        <% if flash[:error] %>
          <div class="notice"><%= flash[:error] %></div>
        <% end %>
        <%= yield -%>
      </div>
    </div>
    <div class="span-24 last">
      Footer
    </div>
  </div>
</body>
