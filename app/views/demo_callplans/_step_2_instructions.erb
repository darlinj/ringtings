<% form_for(:demo_callplan, :url=>"/demo_callplans/generate_full_demo_callplan/#{@callplan.id}",
            :html => { :method => :put }) do |@form| %>
  <div class="step_box">
    So that was OK but we can do better...
  </div>
  <div class="step_box">
    Please fill in the phone number of a handy phone: <br />
    <%= @form.label :phone_number %>
    <%= @form.text_field :phone_number, :size=>20 %><br/>
  </div>
  <%= image_submit_tag "/images/next.png", :id => "next_submit_image" %>
<% end %>

