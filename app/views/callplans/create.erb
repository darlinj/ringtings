<div id="phone_configurator">
  <img src="/images/phone_configurator.png"/>

  <div id="dialplan_details" class="span-17">
    <div id="dialplan_details"> 
      <% form_for(:callplans, :url=>callplan_path(1), :html => { :method => :put }) do |@form| %>
       <div id="try_it_thread">
        <div class="step_box">
          <%= render :partial => 'instructions_step1' %>
          <%= render :partial => 'company_name' %>
        </div>
        <div class="step_box">
          <h3>  Please ring <%= @inbound_number %> now (it's free) </h3>
        </div>
        <%= render :partial => 'instructions_step2' %>
        <%= image_submit_tag "/images/next.png" %> 
      </div>
      <% end %>
    </div>
  </div>
</div>
